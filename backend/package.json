  "dependencies": {
    "@ai-sdk/google": "^3.0.15",
    "@ai-sdk/groq": "^3.0.22",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@sentry/node": "^10.38.0",
    "ai": "^6.0.57",
    "apple-signin-auth": "^2.0.0",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.17.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.2.0",
    "@types/pg": "^8.16.0",
    "eslint": "^9.39.2",
